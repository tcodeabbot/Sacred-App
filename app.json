{
  "expo": {
    "name": "Sacred",
    "slug": "sacred-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "sacred",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d0d0d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.eidos.sacred-mobile-app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.343809534930-qmdjjacb9c2m436rv18ckpu5mo076vgj",
              "fb$(EXPO_PUBLIC_FACEBOOK_APP_ID)"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "FacebookAppID": "$(EXPO_PUBLIC_FACEBOOK_APP_ID)",
        "FacebookDisplayName": "Sacred"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d0d0d"
      },
      "package": "com.sacred.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.sacred.app",
              "host": "oauth2redirect",
              "pathPrefix": "/google"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "./plugins/withModularHeaders.js",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.343809534930-qmdjjacb9c2m436rv18ckpu5mo076vgj"
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "930460469412108",
          "clientToken": "2bbc2f2079743e48a80521388c2d480f",
          "displayName": "Sacred",
          "scheme": "fb930460469412108",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "315ebfb0-3388-4d82-bf83-848ed19f5ff0"
      }
    },
    "owner": "abbot"
  }
}
